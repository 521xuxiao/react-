#home{
    height: 100%;
    .home{
        height: 100%;
        background: url("../../common/img/bg.png") no-repeat 0 0;
        background-size: 100% 100%;
        .home_top{
            height: 10%;
            .home_top_left{
                white-space:nowrap;
                float: left;
                color: #2AA7D3;
                font-size: 2rem;
                padding: 1rem 0 0 1.5rem;
                width: 12.5%;
                .span2{
                    margin-left: 1rem;
                }
            }
            .home_top_middle{
                float: left;
                background: url("../../common/img/title.png") no-repeat 0 0;
                background-size: 100% 100%;
                height: 100%;
                width: 75%;
                margin: 0 auto;
                padding-top: 1rem;
                .home_top_middle_text{
                    width: 19.9rem;
                    display: block;
                    margin: 0 auto;
                    cursor: pointer;
                }
            }
            .home_top_right{
                white-space:nowrap;
                float: left;
                color: #2AA7D3;
                font-size: 2rem;
                padding: 1rem 1.5rem 0 0;
                width: 12.5%;
                overflow: hidden;
                .span4{
                    display: block;
                    float: left;
                    width: 33.3%;
                    text-align: center;
                }
            }
        }
        .home_tab{
            height: 8.6%;
            .ul1{
                display: flex;
                justify-content: space-between;
                color: #FEFEFE;
                width: 60%;
                margin: 0 auto;
                .li1{
                    background: url("../../common/img/nav.png") no-repeat 0 0;
                    background-size: 100% 100%;
                    width: 16rem;
                    height: 4.1rem;
                    text-align: center;
                    line-height: 4.1rem;
                    cursor: pointer;
                    user-select: none;
                }
                .active{
                    background: url("../../common/img/active.png") no-repeat 0 0;
                    background-size: 100% 100%;
                }
            }
        }
        .home_children{
            height: 81.4%;
            .ant-carousel{
                height: 100%;
                .slick-slider{
                    height: 100%;
                    .slick-list{
                        height: 100%;
                        .slick-track{
                            height: 100%;
                            .slick-slide{
                                height: 100%;
                                div{
                                    height: 100%;
                                    .div1{
                                        height: 100%;
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}